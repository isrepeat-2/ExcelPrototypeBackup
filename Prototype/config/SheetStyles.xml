<?xml version="1.0" standalone="yes"?>
<SheetStyles xmlns="urn:excelprototype:presets" version="1">
  <baseSheetStyle priority="100">
    <base backColor="#262626" fontColor="#EBEBEB" showGridlines="false"/>
    <background extraRows="200" extraCols="30"/>
    <grid color="#000000" weight="thin" extraRows="200" extraCols="50"/>
  </baseSheetStyle>

  <outputSheetStyle priority="200" outputTopOffsetRows="4" headerRows="4" viewStartRow="6" errorBannerColumns="8">
    <font name="Times New Roman" size="12"/>
    <rows height="20"/>

    <content color="#EBEBEB" backColor="#222222"/>
    <header color="#68D947" backColor="#222222" bold="true"/>
    <section color="#D7A763" backColor="#161616" bold="true" sectionTitleColumns="2"/>

    <alignment horizontal="center" vertical="center"/>

    <status
      columnName="Status"
      fontColor="#EBEBEB"
      defaultBackColor="#1E1E1E"
      addedBackColor="#2E7D32"
      changedBackColor="#7B1FA2"
      removedBackColor="#B71C1C"/>

    <controlPanel
      title="Quick Search"
      startColumn="1"
      minStartColumn="1"
      offsetColumns="2"
      widthColumns="5"
      heightRows="5"
      topRow="1"
      labelColumns="1"
      valueColumns="2"
      fieldRowSpan="2"
      fieldSpacingRows="0"
      panelColumnWidth="12"
      panelBackColor="#1E1E1E"
      panelBorderColor="#505050"
      titleColor="#D7A763"
      labelColor="#EBEBEB"
      inputBackColor="#262626"
      inputFontColor="#EBEBEB"
      buttonBackColor="#1F5E9C"
      buttonTextColor="#FFFFFF"
      buttonBorderColor="#163F69">
      <fields>
        <field label="Key" inputName="Context.PersonValue" inputConfigKey="Context.PersonValue">
          <button caption="Action 1" macro="ex_UIActions.m_OutputPanelStartSearch_OnClick"/>
          <button caption="Action 2" macro="ex_UIActions.m_OutputPanelStartSearch_OnClick"/>
        </field>
        <field label="Another Key" inputName="Context.RoleValue" inputConfigKey="Context.RoleValue">
          <button caption="Action 3" macro="ex_UIActions.m_OutputPanelStartSearch_OnClick"/>
        </field>
      </fields>
    </controlPanel>
  </outputSheetStyle>
</SheetStyles>
